{
  "compilerOptions": {
    "target": "ES2020",
  "module": "ESNext",
  "moduleResolution": "Node",
  "allowSyntheticDefaultImports": true,
  "resolveJsonModule": true,
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
